<div class="row">
  <% for key in settings.keyboard %>
    <% if key == "#" %>
      <div class="span1" style="height: 90px;">
      </div>
    <% else %>
      <div class="pad span1 disabled">
        <a class="btn" data-shortcut="<%= key %>">
          <span class="name"></span>

          <audio></audio>

          <div class="progress" style="display: none;">
            <div class="bar"></div>
          </div>
        </a>
      </div>
    <% end %>
  <% end %>
</div>